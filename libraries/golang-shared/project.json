{
  "name": "golang-shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libraries/golang-shared",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build ./libraries/golang-shared/..."
      },
      "inputs": ["production", "libraries/golang-shared/**/*"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test ./libraries/golang-shared/..."
      }
    }
  }
}

